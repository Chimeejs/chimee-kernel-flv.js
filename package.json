{
  "name": "chimee-kernel-flv.js",
  "version": "0.2.0",
  "description": "A flv decoder for chimee, using flv.js",
  "main": "lib/index.js",
  "scripts": {
    "precommit": "npm run lint && npm t",
    "prepush": "npm run test",
    "prepublishOnly": "npm run lint && npm t && pkg-ok",
    "build": "npm run b-common && npm run b-es && npm run b-umd && npm run b-min",
    "b-common": "rollup -c build/rollup.config.common.js",
    "b-es": "rollup -c build/rollup.config.es.js",
    "b-umd": "rollup -c build/rollup.config.umd.js",
    "b-min": "rollup -c build/rollup.config.min.js",
    "start": "rollup -c build/rollup.config.dev.js -w",
    "lint": "eslint . --fix && flow",
    "flow": "flow",
    "test": "karma start karma.conf.js",
    "unit": "karma start karma.conf.js --auto-watch"
  },
  "keywords": [
    "flv.js",
    "chimee",
    "player",
    "video",
    "flv"
  ],
  "author": "toxic-johann",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "chimee-helper": "^0.2.10",
    "flv.js": "^1.4.0",
    "toxic-decorators": "^0.3.8"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "chimee-kernel": "^1.4.0",
    "eslint": "^5.8.0",
    "eslint-config-egg": "^7.1.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "flow-bin": "^0.85.0",
    "flowgen": "^1.2.0",
    "husky": "^1.1.3",
    "karma": "^3.1.1",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-coveralls": "^2.1.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-rollup-preprocessor": "^6.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "mocha": "^5.0.0",
    "pkg-ok": "^2.3.1",
    "rollup": "^0.67.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-flow-no-whitespace": "^1.0.0",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.6.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-watch": "^4.3.1",
    "uglify-es": "^3.3.7"
  }
}
